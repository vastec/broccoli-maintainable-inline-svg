{
  "name": "broccoli-misvg",
  "version": "0.2.0",
  "description": "Combines multiple SVG files into one JS object with SVG sprites as string properties",
  "main": "index.js",
  "xo": {
    "globals": [
      "describe",
      "it",
      "afterEach"
    ],
    "rules": {
      "no-new": "off"
    }
  },
  "scripts": {
