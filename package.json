  },
  "homepage": "https://github.com/vastec/broccoli-misvg#readme",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "broccoli-babel-transpiler": "^6.4.3",
    "broccoli-caching-writer": "3.0.3",
    "broccoli-kitchen-sink-helpers": "0.3.1",
    "misvg": "0.2.0",
