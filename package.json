    "babel-preset-es2015": "^6.18.0",
    "broccoli-babel-transpiler": "^5.6.2",
    "broccoli-caching-writer": "3.0.0",
    "broccoli-kitchen-sink-helpers": "0.3.1",
    "misvg": "^0.1.0",
